@page "/projects/{id}/{organizationId}/settings"
@inherits ProjectBase

@using EstimatorX.Client.Pages.Projects.Components
@using EstimatorX.Client.Repositories
@using EstimatorX.Client.Stores

<PageTitle>Project @Model?.Name - Settings - EstimatorX</PageTitle>

<ProjectContainer Id="@Id" OrganizationId="@OrganizationId" TStore="ProjectStore" TRepository="ProjectRepository" TModel="Project">
    <SettingsEditor TStore="ProjectStore" TRepository="ProjectRepository" TModel="Project" />
</ProjectContainer>
