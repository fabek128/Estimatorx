@page "/templates/{id}/{organizationId}/settings"
@inherits TemplateBase

@using EstimatorX.Client.Pages.Projects.Components
@using EstimatorX.Client.Pages.Templates.Components
@using EstimatorX.Client.Repositories
@using EstimatorX.Client.Stores

<PageTitle>Template @Model?.Name - Settings - EstimatorX</PageTitle>

<TemplateContainer Id="@Id" OrganizationId="@OrganizationId" TStore="TemplateStore" TRepository="TemplateRepository" TModel="Template">
    <SettingsEditor TStore="TemplateStore" TRepository="TemplateRepository" TModel="Template" />
</TemplateContainer>
