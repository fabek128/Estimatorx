<configuration>
  <configSections />
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Changed at Deployment -->
    <add key="Google:ClientId" value="" />
    <add key="Google:ClientSecret" value="" />
    <add key="Microsoft:ClientId" value="" />
    <add key="Microsoft:ClientSecret" value="" />
    <add key="Twitter:ConsumerKey" value="" />
    <add key="Twitter:ConsumerSecret" value="" />
    <add key="Facebook:ApplicationId" value="" />
    <add key="Facebook:ApplicationSecret" value="" />
    <add key="GitHub:ClientId" value="" />
    <add key="GitHub:ClientSecret" value="" />
  </appSettings>

  <connectionStrings>
    <!-- Changed at Deployment -->
    <add name="EstimatorxMongo" connectionString="mongodb://localhost/estimatorx" />
  </connectionStrings>

  <system.net>
    <mailSettings>
      <!-- Changed at Deployment -->
      <smtp deliveryMethod="SpecifiedPickupDirectory" from="noreply@estimatorx.com">
        <network host="localhost" port="25" />
        <specifiedPickupDirectory pickupDirectoryLocation="C:\inetpub\smtp" />
      </smtp>
    </mailSettings>
  </system.net>

  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
    <customErrors mode="RemoteOnly" redirectMode="ResponseRewrite" defaultRedirect="~/500.aspx">
      <error statusCode="404" redirect="~/404.aspx" />
      <error statusCode="500" redirect="~/500.aspx" />
    </customErrors>
    <!-- Changed at Deployment -->
    <machineKey decryption="AES" validation="SHA1" decryptionKey="E9525581D876D817CA220116D785B6C18CB393BD96A2A2F4" validationKey="4A9FE30505378B06F6E382F95BE30460316A3AB92F5FB3069CDE0C278E7202E078BBED1FD73681E0069170394D9477953FC91AB995B15099846A0C48903C60C6" />
  </system.web>

  <system.webServer>
    <modules>
      <remove name="FormsAuthenticationModule" />
    </modules>
    <httpErrors errorMode="DetailedLocalOnly">
      <remove statusCode="404" />
      <remove statusCode="500" />
      <error statusCode="404" path="404.html" responseMode="File" />
      <error statusCode="500" path="500.html" responseMode="File" />
    </httpErrors>
    <staticContent>
      <remove fileExtension=".ts" />
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".ts" mimeType="text/x-typescript" />
      <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
